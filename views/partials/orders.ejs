      <table data-js="orders-list" id="table-orders" >
        <tr id="tr-orders">
          <th>CÓDIGO</th>
          <th>PROJETO</th>
          <th>REVENDA</th>
          <th>USUÁRIO</th>
          <th>CLIENTE FINAL</th>
          <th>VALOR</th>
          <th>INÍCIO</th>
          <th>PRAZO</th>
          <th>SITUAÇÃO</th>
        </tr>
        <%ordersList.forEach(order => {%>
          <tr class="order-table-rows">
            <td><%= order._id %></td>
            <td><%= order.title %></td> 
            <td><%= order.company %></td>
            <td><%= order.requester %></td>
            <td><%= order.finalClient %></td>
            <td><%= order.price %></td>
            <td><%= order.startedAt.toLocaleDateString('pt-BR', {timeZone: 'UTC'}) %> - <%= order.startedAt.toLocaleDateString('pt-BR', {timeStyle: 'short'}) %></td>
            <td><%= order.deadline %></td>
            <td><%= order.stage %></td>
            <!-- <td><%= order.closedAt %></td> -->
          </tr>          
        <%})%>        
      </table>

      <!--FORMULÁRIO DE PEDIDO-->
      <div class="modal-open-order">
        <form action="/orders" method="post" id="orders-form" class="fm-nw-order" enctype="multipart/form-data">
          <p>Novo Pedido</p>
          <div class="fm-group g1">
            <label for="">Cliente</label>
            <input type="text" name="finalClient" id="finalClient">
          </div>
          <div class="fm-group g1">
            <label for="">Nome do Projeto</label>
            <input type="text" name="title" id="title">
          </div>             
          <div class="fm-group g3">
            <label for="">Observações</label>
            <textarea rows="10" name="obs" id="obs"></textarea>
          </div>         
          <div class="fm-group g4"> 
            <label for="">Arquivo do projeto(Fulana_de_tal.rar)</label> 
            <input type="file" name="attachment" id="attachment">     
          </div> 
          <div class="g5">
            <a class="btn-cancel" href="orders">Cancelar</a>  
            <button type="submit" class="btn-black">Enviar</button>
          </div>      
        </form> 
      </div> 

      
    </main>
  <script src="js/orders.js"></script>




